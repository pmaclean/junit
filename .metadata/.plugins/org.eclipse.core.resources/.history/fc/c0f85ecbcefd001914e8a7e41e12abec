package example2;

public class DuckHome {
	// Body of water where the duck lives - Walden Pond, North Park Lake, Golden Pond
	public DuckHome(String waterBody)
	{
		if (waterBody==null) throw new IllegalArgumentException("Unexpected null pointer receive as argument named 'waterBody'.");
		this.waterBody = waterBody;
	}
	public String getLocation() {
		return waterBody;
	}
	public void setLocation(String waterBody) {
		if (waterBody==null) throw new IllegalArgumentException("Unexpected null pointer receive as argument named 'waterBody'.");
		this.waterBody = waterBody;
	}
	private String waterBody;
	
	/*	
	private String county;
	private String state;
	...
	*/
}
