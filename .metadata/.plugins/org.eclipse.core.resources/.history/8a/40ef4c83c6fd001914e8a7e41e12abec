package example2;

public class DuckHabitat {
	public DuckHabitat() 
	{ 
		this("");
	}
	public DuckHabitat(String street1)
	{
		this.setStreet1(street1);
		this.setStreet2(street2);
		this.setCity(city);
		this.setState(state);
		this.setZip(zip);
	}
	public String getStreet1() {
		return street1;
	}
	public void setStreet1(String street1) {
		if (street1==null) throw new IllegalArgumentException("Unexpected null pointer receive as argument named 'street1'.");
		this.street1 = street1;
	}
	private String street1;
}
