package example1;

// Calculate total duck weight for weigh-in.  
public class DuckWeighIn1 {

	// Find the total weights of ducks on team
	public float Sum(Float[] duckWeights)
	{
		float totalWeight = 0;
		for (int i=1; i<duckWeights.length; i++)
		{
			totalWeight += duckWeights[i];
		}
		return totalWeight;
	}
}
