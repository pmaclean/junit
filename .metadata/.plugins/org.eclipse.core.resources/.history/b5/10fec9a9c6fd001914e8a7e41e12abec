package example2;

public class DuckHabitat {
	public DuckHabitat() 
	{ 
		this("");
	}
	public DuckHabitat(String habitatName)
	{
		this.setHabitatName(habitatName);
	}
	public String getHabitatName() {
		return habitatName;
	}
	public void setHabitatName(String habitatName) {
		if (habitatName==null) throw new IllegalArgumentException("Unexpected null pointer receive as argument named 'habitatName'.");
		this.habitatName = habitatName;
	}
	private String habitatName;
}
