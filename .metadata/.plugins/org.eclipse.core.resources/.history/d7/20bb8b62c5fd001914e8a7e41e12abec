package example1;

public class DuckWeighIn2 {

	// Return the sum of all values in array of Float
	public Float Sum(Float[] values)
	{
		if (values == null) throw new IllegalArgumentException("Unexpected null encountered with argument 'values'");

		Float accumulator = 0f;
		for (int i=0; i<values.length; i++)
		{
			accumulator += values[i];
		}
		return accumulator;
	}
}
